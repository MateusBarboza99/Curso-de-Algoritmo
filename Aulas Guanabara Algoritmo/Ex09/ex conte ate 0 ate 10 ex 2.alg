algoritmo "Contede10Ate0"
var
  valor, contador: inteiro
inicio
    contador <- 0
    Escreva ("Quer contar ate quando?")
    leia (valor)
    Enquanto (contador <= valor) faca
        Escreval(contador)
        contador <- contador + 1
    FimEnquanto
    Escreval ("Terminei de Contar ")
fimalgoritmo
