algoritmo "CalculeIdade"
var
   Ano, Nasc, Idade:Inteiro

inicio
     Escreva ("Em que ano estamos? ")
     leia(Ano)
     Escreva ("Em que ano voce nasceu: ")
     Leia(Nasc)
     Idade <- ano - Nasc
     Escreval (" Em " , ano ," voce tera", idade ," ano ")
     Se (Idade >= 21) entao
       Escreval (" E ja tera atingido a maior idade. ")
      FIMSE
fimalgoritmo
